  "name": "egg-multiple-page-example",
  "version": "1.0.0",
  "description": "一个使用egg+webpack4结合开发的多页面脚手架demo，包含HMR、es6、sass、vue、tree-shaking、lint-staged等特性，支持通过配置的方式来构建项目，项目结构简单实用。",
  "dependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@babel/polyfill": "7.0.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/runtime": "^7.6.0",
    "babel-eslint": "^10.0.3",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "core-js": "^3.2.1",
    "css-hot-loader": "^1.4.4",
    "egg": "^2.15.1",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.11.0",
    "egg-mock": "^3.21.0",
    "egg-scripts": "^2.11.0",
    "egg-static": "^2.2.0",
    "egg-view-nunjucks": "^2.2.0",
    "eslint-plugin-vue": "^5.2.3",
    "file-loader": "^4.2.0",
    "husky": "^3.0.7",
    "lint-staged": "^9.4.0",
    "lodash": "^4.17.15",
